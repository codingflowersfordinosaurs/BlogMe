<div class="container">
  <div class="row">
    <div class="col-lg-12">
      <h1>All Pages</h1>

      <div class="people_list">
        {{#each pages}}
        <div class="card">
          <div class="card-body">
            <h3>
              {{this.title}}
            </h3>
            <p>
              {{this.description}}
            </p>
            <p>
              {{this.createdAt}}
            </p>
            <a href="/pages_edit/{{this.id}}">
              Edit
            </a>
            <a href="/pages_edit/{{this.id}}">
              Delete
            </a>
          </div>
        </div>
        {{/each}}
      </div>
    </div>
  </div>
</div>